{
	"name": "coup-cli",
	"version": "1.0.0",
	"description": "The COUP game implemented in the CLI",
	"homepage": "https://github.com/dominikwilkowski/coup-cli",
	"scripts": {
		"test:format": "prettier --list-different \"**/*.{js,md,mdx,json,html}\"",
		"test:code": "node test/test.js",
		"test": "npm run test:format && npm run test:code",
		"format": "prettier --write \"**/*.{js,md,mdx,json,html}\"",
		"play": "node src/bin.js play",
		"loop": "node src/bin.js loop"
	},
	"main": "./src/index.js",
	"bin": {
		"coup": "./src/bin.js"
	},
	"keywords": [
		"cli",
		"coup",
		"game"
	],
